}
.btnSearch {
  margin-top: 20rpx;
  background-color: #FFCB2A;
  color: white;
  font-size: 0.9rem;
}
.datePicker {
  position: absolute;
  top: 350rpx;
  left: 50rpx;
}
.divLine {
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  width: 610rpx;
  height: 1px;
  background:rgba(189, 186, 186, 0.918);
}
.moreHotel {
  width: 690rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.moreHotelBtn {
  position: absolute;
  top: 0rpx;
