.top {
  height: 870rpx;
}
.searchTableOutside {
  position: absolute;
  box-shadow: 0rpx 0rpx 3rpx 2rpx gainsboro;
  top: 350rpx;
  left: 30rpx;
  width: 690rpx;
  background-color: white;
  border-radius: 5rpx;
}
.searchTableInside {
  margin-right: 40rpx;
  margin-left: 40rpx;
  margin-bottom: 20rpx;
  background-color: white;
}
.leftText {
  margin-top:20rpx; 
  width: 200rpx;
  display: inline-block;
  font-size: 1rem;
  color: rgb(15, 15, 15);
}
.rightText {
  margin-top:20rpx; 
  width: 400rpx;
  display: inline-block;
  text-align: right;
  font-size: 0.8rem;
  color:rgba(189, 186, 186, 0.918);
}
.searchRow {
  background-color: white;
  font-size: 0.75rem;
